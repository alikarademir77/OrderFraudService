{
  "swagger": "2.0",
  "info": {
    "description": "REST API for rertrieving details of various order types.",
    "version": "1",
    "title": "Order Details Service REST API",
    "contact": {
      "name": "EComm Orders Team",
      "url": "https://confluence.ca.bestbuy.com/display/OR/Orders",
      "email": "eCommCAOrders@bestbuy.com"
    }
  },
  "host": "localhost:8080",
  "basePath": "/orderdetails",
  "tags": [
    {
      "name": "order-details-controller",
      "description": "Operations for retrieving details of an FS Order"
    }
  ],
  "paths": {
    "/v1/categories": {
      "get": {
        "tags": [
          "order-details-controller"
        ],
        "summary": "View list of valid categories that can be used to filter details of an FS Order",
        "operationId": "listFSOrderDetailsCategoriesUsingGET",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "ordertype",
            "in": "query",
            "description": "Type of order for which categories will be retrieved.",
            "required": true,
            "type": "string",
            "enum": [
              "FSORDER"
            ]
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Operation",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "schema": {
              "$ref": "#/definitions/ErrorDetail"
            }
          },
          "500": {
            "description": "Internal Server Error",
            "schema": {
              "$ref": "#/definitions/ErrorDetail"
            }
          }
        }
      }
    },
    "/v1/fsorders/{fsOrderId}": {
      "get": {
        "tags": [
          "order-details-controller"
        ],
        "summary": "View details of a specific FS Order by id",
        "operationId": "readFSOrderDetailsUsingGET",
        "produces": [
          "application/json;charset=UTF-8"
        ],
        "parameters": [
          {
            "name": "fsOrderId",
            "in": "path",
            "description": "FS Order Id",
            "required": true,
            "type": "string"
          },
          {
            "name": "categories",
            "in": "query",
            "description": "Comma separated list of sub elements of the order detail to retrieve. Elements can be specified by path of the element in object graph starting from root using dot notation.",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Operation",
            "schema": {
              "$ref": "#/definitions/FSOrder"
            }
          },
          "400": {
            "description": "Bad Request",
            "schema": {
              "$ref": "#/definitions/ErrorDetail"
            }
          },
          "404": {
            "description": "Resource Not Found",
            "schema": {
              "$ref": "#/definitions/ErrorDetail"
            }
          },
          "500": {
            "description": "Internal Server Error",
            "schema": {
              "$ref": "#/definitions/ErrorDetail"
            }
          }
        }
      }
    }
  },
